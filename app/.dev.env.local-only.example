# OMI App - Local-Only Mode Configuration
# Copy this to .dev.env for local-only operation with PAI-Bridge

# PAI-Bridge local mode - bypasses OMI backend entirely
USE_LOCAL_ONLY=true

# PAI-Bridge WebSocket URL (your local Whisper + Claude server)
# Use your Tailscale IP or localhost
LOCAL_BRIDGE_URL=ws://100.91.226.93:8000

# These can be empty/dummy values for local-only mode
API_BASE_URL=http://100.91.226.93:8000/
OPENAI_API_KEY=
MIXPANEL_PROJECT_TOKEN=
GROWTHBOOK_API_KEY=

# Disable web auth (not needed for local mode)
USE_WEB_AUTH=false
USE_AUTH_CUSTOM_TOKEN=false

# Optional: Google Maps for location features (get from Google Cloud Console)
GOOGLE_MAPS_API_KEY=

# Optional: Intercom for support (not needed for local mode)
INTERCOM_APP_ID=
INTERCOM_IOS_API_KEY=
INTERCOM_ANDROID_API_KEY=

# Optional: Google OAuth (not needed for local mode)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
